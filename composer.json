{
    "name": "becklyn/entity-admin",
    "type": "symfony-bundle",
    "description": "Provides functionality for handling entities in an admin backend.",
    "require": {
        "php": "^7.4",
        "becklyn/rad-bundle": "^7.5",
        "symfony/cache-contracts": "^2.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/routing": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0.7"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-next": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\EntityAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\EntityAdmin\\": "tests/"
        }
    }
}
