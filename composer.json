{
    "name": "becklyn/entity-admin",
    "description": "Provides functionality for handling entities in an admin backend.",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/entity-admin",
    "require": {
        "php": ">=7.4",
        "becklyn/rad": "^8.6.0",
        "becklyn/rad-bundles": "^1.1.0",
        "symfony/cache-contracts": "^v2.1.3 || ^3.0.0",
        "symfony/http-foundation": "^5.4.3 || ^6.0.3",
        "symfony/http-kernel": "^5.4.4 || ^6.0.4",
        "symfony/routing": "^5.4.3 || ^6.0.3",
        "symfony/translation-contracts": "^v2.4.1 || ^3.0.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.4.3 || ^6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\EntityAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\EntityAdmin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-next": "2.x-dev"
        }
    }
}
